    xmlns:petrinet="petrinet"
    xsi:schemaLocation="petrinet petrinet.ecore">
  <place name="p1">
    <tokens>1</tokens>
    <tokens>2</tokens>
  </place>
  <place name="p2">
    <tokens>1</tokens>
  </place>
  <place name="p3">
    <tokens>0</tokens>
  </place>
  <place name="p4">
