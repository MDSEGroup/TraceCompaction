<?xml version="1.0" encoding="UTF-8"?>
<petrinet:Net
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petrinet="petrinet"
    xsi:schemaLocation="petrinet model/domain%20model.ecore">
  <place name="P1"
      tokens="1"/>
  <place name="P2"
      tokens="1"/>
  <place name="P3"/>
  <place name="P4"/>
  <transition
      input="//@place.0"
      output="//@place.2"
      name="T1"/>
  <transition
      input="//@place.1 //@place.2"
      output="//@place.3"
      name="T2"/>
</petrinet:Net>